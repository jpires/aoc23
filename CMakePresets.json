{
  "version": 4,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "dev",
      "binaryDir": "${sourceDir}/build/dev",
      "generator": "Ninja",
      "toolchainFile":  "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
      "environment": {
        "VCPKG_ROOT" : "/home/app/vcpkg"
      },
      "vendor": {
        "jetbrains.com/clion": {
          "toolchain": "aoc23"
        }
      }
    }
  ]
}
